{
  "name": "@denali-design/ember",
  "version": "0.0.0-development",
  "description": "Ember.JS component library for the Denali CSS Framework",
  "keywords": [
    "ember-addon"
  ],
  "repository": "git@github.com:denali-design/denali-ember.git",
  "license": "MIT",
  "author": "denali@verizonmedia.com",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "broccoli-funnel": "^3.0.1",
    "broccoli-merge-trees": "^4.1.0",
    "denali-css": "^2.0.0",
    "denali-icon-font": "^1.4.1",
    "ember-arg-types": "^0.1.0",
    "ember-cli-babel": "^7.19.0",
    "ember-cli-htmlbars": "^4.3.1",
    "ember-composable-helpers": "^4.0.0",
    "ember-truth-helpers": "^2.1.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.18.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.18.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.7.0",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-ember": "^8.4.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^4.2.5",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "qunit-dom": "^1.2.0",
    "sass": "^1.25.0",
    "semantic-release": "^17.0.7",
    "validate-commit": "^3.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "validate-commit-msg"
    }
  },
  "release": {
    "repositoryUrl": "git@github.com:denali-design/denali-ember.git",
    "branches": [
      "next",
      {
        "name": "master",
        "channel": "alpha",
        "prerelease": "alpha"
      }
    ]
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
