{
  "name": "@denali-design/ember",
  "version": "0.0.0-development",
  "description": "Ember.JS component library for the Denali CSS Framework",
  "keywords": [
    "ember-addon"
  ],
  "repository": "git@github.com:denali-design/denali-ember.git",
  "license": "MIT",
  "author": "denali@verizonmedia.com",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "npm run storybook",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "semantic-release": "semantic-release",
    "build-storybook": "ember build && build-storybook -s dist",
    "storybook": "ember serve & start-storybook -p 9001 -s dist",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "dependencies": {
    "@glimmer/component": "^1.0.2",
    "@glimmer/tracking": "^1.0.2",
    "broccoli-funnel": "^3.0.1",
    "broccoli-merge-trees": "^4.1.0",
    "denali-css": "^2.1.0",
    "denali-icon-font": "^1.6.0",
    "ember-arg-types": "^0.2.1",
    "ember-cli-babel": "^7.22.1",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-composable-helpers": "^4.3.0",
    "ember-truth-helpers": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@ember/optional-features": "^2.0.0",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-knobs": "^6.0.21",
    "@storybook/addon-storysource": "^6.0.21",
    "@storybook/ember": "^6.0.21",
    "@storybook/ember-cli-storybook": "^0.2.1",
    "@storybook/storybook-deployer": "^2.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-ember-modules-api-polyfill": "^3.1.1",
    "babel-plugin-htmlbars-inline-precompile": "^4.2.0",
    "broccoli-asset-rev": "^3.0.0",
    "core-js": "^3.6.5",
    "ember-cli": "~3.21.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.21.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.11.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.8.0",
    "eslint-plugin-ember": "^8.13.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^4.2.5",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "qunit-dom": "^1.4.0",
    "sass": "^1.25.0",
    "semantic-release": "^17.0.7",
    "validate-commit": "^3.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "validate-commit-msg"
    }
  },
  "release": {
    "repositoryUrl": "git@github.com:denali-design/denali-ember.git",
    "branches": [
      "next",
      {
        "name": "master",
        "channel": "alpha",
        "prerelease": "alpha"
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "storybook-deployer": {
    "gitUsername": "denali-bot",
    "gitEmail": "denali@verizonmedia.com",
    "commitMessage": "Deploy Storybook to GitHub Pages"
  }
}
